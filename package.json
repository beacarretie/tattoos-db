{
   "name": "tattoos-db",
   "version": "1.0.0",
   "description": "",
   "main": "dist/server.js",
   "scripts": {
      "build": "tsc",
      "start": "tsc && node dist/server.js",
      "dev": "nodemon --files ./src/server.ts",
      "db:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/database/data-source.ts",
      "db:migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
      "db:seed": "npx ts-node ./src/database/seeders/dbSeeder.ts",
      "db:refresh": "npm run db:drop && npm run db:migrate && npm run db:seed"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@faker-js/faker": "^8.4.1",
      "bcrypt": "^5.1.1",
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.18.3",
      "factorial": "^2.0.0",
      "jsonwebtoken": "^9.0.2",
      "mysql2": "^3.9.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/jsonwebtoken": "^9.0.6",
      "@types/node": "^16.11.10",
      "nodemon": "^3.1.0",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   }
}
